{
  "name": "hummingbird",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js --presets env",
    "start:watch": "nodemon index.js --exec babel-node --presets env"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.5.0"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "nodemonConfig": {
    "ext": "js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthtonerainbow/hummingbird.git"
  },
  "author": "Earth Tone Rainbow Games LLC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.2"
  }
}
